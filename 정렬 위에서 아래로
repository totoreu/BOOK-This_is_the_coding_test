#include <iostream>
#include <vector>
#include <algorithm>

using namespace std;

void solution(int n, vector<int> arr) {
    sort(arr.begin(), arr.end());
    for (int i = arr.size() - 1; i >= 0; --i)
        cout << arr[i] << "\n";
}

int main() {
    
    int n = 0;
    cin >> n;

    vector<int> arr(n, 0);
    for (auto& num : arr)
        cin >> num;

    solution(n, arr);

    return 0;
}
