#include <iostream>
#include <map>
#include <algorithm>

using namespace std;

void solution(map<int, string> m) {
    for (auto& iter : m)
        cout << iter.second << " ";
}

int main() {
    
    int n = 0;
    cin >> n;

    map<int, string> m;
    for (int i = 0; i < n; ++i) {
        int score = 0;
        string name;
        cin >> name >> score;
        m.insert({ score, name });
    }

    solution(m);

    return 0;
}
